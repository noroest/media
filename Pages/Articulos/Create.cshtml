@page
@model ProyeArticulos.Pages.Articulos.CreateModel

@{
    ViewData["Title"] = "Crear";
}

<h1>Crear</h1>

<h4>Articulo</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Articulo.nombre" class="control-label"></label>
                <input asp-for="Articulo.nombre" class="form-control" />
                <span asp-validation-for="Articulo.nombre" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Articulo.costo" class="control-label"></label>
                <input asp-for="Articulo.costo" class="form-control" />
                <span asp-validation-for="Articulo.costo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Articulo.precio" class="control-label"></label>
                <input asp-for="Articulo.precio" class="form-control" />
                <span asp-validation-for="Articulo.precio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
            </div>
        </form>
        <img src="data:image/jpeg;base64," width="100px" height="100px" />
    </div>
</div>

<div>
    <a asp-page="Index">Atras, a la lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
